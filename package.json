{
  "name": "ts-mobx-react-starter-kit",
  "version": "1.0.0",
  "description": "Mobx & React starter kit written in Typescript.",
  "main": "src/app.ts",
  "scripts": {
    "test": "karma start",
    "test-once": "COVERAGE=true karma start --single-run --log-level warn",
    "start": "webpack-dev-server --progress --inline --hot",
    "postinstall": "typings install",
    "clean": "rm -rf ./dist ./coverage",
    "build": "npm run clean && npm run lint && npm run test-once && NODE_ENV=production webpack -p --progress",
    "lazyBuild": "npm run clean && NODE_ENV=production NODE_API=staging webpack -p --progress",
    "lint": "npm run lint-ts && npm run lint-sass",
    "lint-ts": "tslint ./src/*.ts*",
    "lint-sass": "sass-lint -v -q",
    "cloneDist": "rm -rf ./docs && mkdir ./docs && cp -a ./dist/. ./docs/"
  },
  "keywords": [
    "mobx",
    "react",
    "typescript",
    "starter",
    "kit"
  ],
  "author": "Igor Šćekić <igor.scekic2@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "awesome-typescript-loader": "^1.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.23.1",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.21.0",
    "image-webpack-loader": "^3.2.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-webpack": "^1.7.0",
    "mobx-react-devtools": "^4.2.0",
    "node-sass": "^3.7.0",
    "noop-loader": "^1.0.0",
    "null-loader": "^0.1.1",
    "postcss-cssnext": "^2.6.0",
    "postcss-loader": "^0.9.1",
    "postcss-scss": "^0.1.8",
    "react-addons-test-utils": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "redux-cli": "^1.9.0",
    "resolve-url-loader": "^1.6.1",
    "sass-lint": "^1.7.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "tslint": "^3.11.0",
    "typescript": "^1.8.10",
    "typings": "^1.0.5",
    "url-loader": "^0.5.8",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "mobx": "^2.2.2",
    "mobx-react": "^3.3.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-ga": "^2.1.2",
    "react-swipe": "^5.0.6",
    "smoothscroll": "^0.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Torwori/ts-mobx-react-starter-kit.git"
  },
  "bugs": {
    "url": "https://github.com/Torwori/ts-mobx-react-starter-kit/issues"
  },
  "homepage": "https://github.com/Torwori/ts-mobx-react-starter-kit#readme"
}
